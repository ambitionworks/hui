    "keywords": ["laravel", "blade", "components", "tailwind"],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AmbitionWorks\\Hui\\": "src/"
        }
